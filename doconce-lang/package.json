{
    "name": "doconce-lang",
    "displayName": "doconce-lang",
    "description": "Syntax highlighting for doconce",
    "version": "0.1.1",
    "publisher": "henriasv",
    "repository" : "https://github.com/henriasv/vscode-doconce-support",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "doconce",
            "aliases": ["doconce", "doconce"],
            "extensions": [".do.txt", ".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "doconce",
            "scopeName": "source.doconce",
            "path": "./syntaxes/doconce.tmLanguage.json"
        }]
    }
}